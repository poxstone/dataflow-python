    "version": "0.2.0",
    "configurations": [
        {
            "name": "current_file",
            "type": "debugpy",
            "request": "launch",
            "program": "${file}",
            "cwd": "${workspaceFolder}/src/beam-started"
        },
        {
            "name": "00_basics",
            "type": "debugpy",
            "request": "launch",
            "cwd": "${workspaceFolder}/src/beam-started",
            "program": "${workspaceFolder}/src/beam-started/00_basics.py",
        },
        {
            "name": "01_read_write_csv",
            "type": "debugpy",
            "request": "launch",
            "cwd": "${workspaceFolder}/src/beam-started",
            "program": "${workspaceFolder}/src/beam-started/01_read_write_csv.py",
        },
        {
            "name": "02_word_count",
            "type": "debugpy",
            "request": "launch",
            "cwd": "${workspaceFolder}/src/beam-started",
            "program": "${workspaceFolder}/src/beam-started/02_word_count_template.py",
            "args": [
                "--input=./data/kinglear_part_*.txt", "--output=./data_outputs/kinglear_count"
            ],
            "env": {
                "GOOGLES_APPLICATION_CREDENTIALS": "${workspaceFolder}/credentials.json"
            }
        },
        {
            "name": "bq_to_fs_main",
            "type": "debugpy",
            "request": "launch",
            "cwd": "${workspaceFolder}/src/dataflow-bq-to-firestore",
            "program": "${workspaceFolder}/src/dataflow-bq-to-firestore/bq_to_fs_main.py",
            "args": [
                "--project=bluetab-colombia-data-qa",
                "--temp_location=gs://bluetab-colombia-data-qa_dataflow01/temp/",
                "--input_bq_table_id=bluetab-colombia-data-qa.thelook_ecommerce.users",
                "--output_fs_project_id=bluetab-colombia-data-qa",
                "--output_fs_db_id=fs-db-uscentral1",
                "--output_fs_collection_name=users",
                "--output_fs_id_field=id"
            ],
            "env": {
                "GOOGLES_APPLICATION_CREDENTIALS": "${workspaceFolder}/credentials.json"
            }
        },
        {
            "name": "test-firestore",
            "type": "debugpy",
            "request": "launch",
            "cwd": "${workspaceFolder}/src/dataflow-bq-to-firestore/",
            "program": "${workspaceFolder}/src/dataflow-bq-to-firestore/test-firestore.py",
            "args": [],
            "env": {
                "GOOGLES_APPLICATION_CREDENTIALS": "${workspaceFolder}/credentials.json"
            }
        }
    ]
}